package com.varxyz.jv200.mod006;

public class NoteBook extends Product{
	
	public NoteBook(String name) {
		super(name);
	}
	
	public double getPrice() {
		return 25000.0;
	}
	
	public String getPlatform() {
		String pf = null;
		if(getName().startsWith("Android")) {
			pf = "Android";
		}else {
			pf = "Apple";
		}
		return pf;
	}
}
