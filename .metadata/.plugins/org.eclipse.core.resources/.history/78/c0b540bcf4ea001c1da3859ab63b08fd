package com.varxyz.jv200.mode006;

public class EmployeeTest {
	public static void main(String[] args) {
		Employee e = new Employee();
		e.name = "유비";
		e.salary = 200.5;
		System.out.println(e.getDetails());
		Manager m = new Manager();
		m.name = "장비";
		m.salary = 500;
		m.department = "장군";
		System.out.println(m.getDetails());
	}
}
