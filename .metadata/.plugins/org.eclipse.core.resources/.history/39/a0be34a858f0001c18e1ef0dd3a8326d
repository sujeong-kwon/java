package com.varxyz.banking.domain;

import java.util.List;

public class Customer {
	protected String name; // 이름
	protected String ssn; // 주민번호
	protected String phone; // 연락처
	protected String customerId; // 고객id
	protected String passwd; // 비밀번호
	protected List<String> Account; // 보유계좌 List<Account> or Collection<Account>

	public Customer(String name, String ssn, String phone, String customerId, String passwd, List<String> Account) {
		this.name = name;
		this.ssn = ssn;
		this.phone = phone;
		this.customerId = customerId;
		this.passwd = passwd;
		this.Account= Account;	
	}
	
	public String getName(){
		return name;
	}
	
	public String getSsn() {
		return ssn;
	}
	
	public String getPhone() {
		return phone;
	}
	
	public String getCustomerId() {
		return customerId;
	}
	
	public String getPasswd() {
		return passwd;
	}
	
	public List<String> getAccount(){
		return Account;
	}
	
}
